{
	"name": "@alphainsiderinc/n8n-nodes-alphainsider",
	"version": "1.0.2",
	"description": "Official community node to connect n8n to the Alpha Insider API.",
	"license": "MIT",
	"author": {
		"name": "AlphaInsider"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AlphaInsider/n8n-nodes-alphainsider"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "npm run clean && tsc && npm run copy:svg",
		"copy:svg": "cp alphaLogo.svg dist/nodes/AlphaInsider/alphaLogo.svg",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/AlphaInsiderApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/AlphaInsider/AlphaInsider.node.js"
		]
	},
	"peerDependencies": {
		"n8n-workflow": "^2.4.0"
	},
	"devDependencies": {
		"typescript": "^5.8.3"
	}
}
